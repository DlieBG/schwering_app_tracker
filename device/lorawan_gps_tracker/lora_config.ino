// Least Significant Bit
static const u1_t PROGMEM APPEUI[8]= { 0x9B, 0x50, 0xF6, 0xDA, 0xC5, 0xF9, 0x81, 0x60};
void os_getArtEui (u1_t* buf) { memcpy_P(buf, APPEUI, 8);}

// Least Significant Bit
static const u1_t PROGMEM DEVEUI[8]= { 0xCC, 0x02, 0x8F, 0x5F, 0x50, 0xF9, 0x81, 0x60 };
void os_getDevEui (u1_t* buf) { memcpy_P(buf, DEVEUI, 8);}

// Most Significant Bit
static const u1_t PROGMEM APPKEY[16] = { 0x08, 0x75, 0x8B, 0x81, 0x5C, 0xC3, 0x30, 0x4A, 0x86, 0xF0, 0x12, 0x42, 0xDB, 0xAB, 0x47, 0xA8 };
void os_getDevKey (u1_t* buf) {  memcpy_P(buf, APPKEY, 16);}